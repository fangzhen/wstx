<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xmlns:soap="http://cxf.apache.org/bindings/soap"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
http://cxf.apache.org/bindings/soap http://cxf.apache.org/schemas/configuration/soap.xsd
http://cxf.apache.org/jaxws
http://cxf.apache.org/schemas/jaxws.xsd">

	<bean id="EPRConfiguration" class="info.fzhen.wstx.util.EPRConfiguration">
		<property name="host"><value>http://localhost:28080/wstx-coor/services/</value></property>
		<property name="activationService"><value>activation</value></property>
		<property name="registrationService"><value>registration</value></property>
	</bean>
	<bean id="coordinatorContext" class="info.fzhen.wstx.coordinator.CoordinatorContext">
		<property name="eprConfiguration"><ref bean="EPRConfiguration"/></property>
	</bean>
	<bean id="coordinator" class="info.fzhen.wstx.coordinator.CoordinatorManager">
		<property name="coordinatorContext"><ref bean="coordinatorContext"></ref></property>
	</bean>
	
</beans>
